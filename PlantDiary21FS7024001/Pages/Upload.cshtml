@page
@model PlantDiary21FS7024001.Pages.UploadModel
@{
    ViewData["Title"] = "Consume XML";
    var result = ViewData["Result"];
}

<h1>Consume XML</h1>
<p>Upload your Plant XML File!</p>
<form method="post" enctype="multipart/form-data">
    <input type="file" asp-for="Upload"/>
    <input type="submit"/>
</form>
<p>Result: @result</p>